{
  "name": "@match-lock/config-editor",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "./dev.sh",
    "dev:vite": "vite",
    "dev:tauri": "cargo tauri dev",
    "dev:simple": "run-p dev:vite dev:watch",
    "dev:watch": "sleep 4 && echo 'ðŸ”„ Watching for changes... Press Ctrl+C to stop, then run: npm run build && npm run start'",
    "prepare": "npm run build:renderer",
    "build": "run-s build:renderer build:ipc build:tauri build:chmod",
    "build:ipc": "cd src-tauri/ipc && npm install && npm run build",
    "build:tauri": "cargo tauri build",
    "build:chmod": "chmod +x \"src-tauri/target/release/bundle/appimage/MatchLock Prep App_0.0.0_amd64.AppImage\"",
    "build:renderer": "rimraf ./dist && tsc -b && vite build",
    "start": "\"./src-tauri/target/release/bundle/appimage/MatchLock Prep App_0.0.0_amd64.AppImage\"",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@match-lock/shared": "file:../shared",
    "@tauri-apps/api": "^2.6.2",
    "@tauri-apps/plugin-dialog": "^2.0.0",
    "@tauri-apps/plugin-fs": "^2.0.0",
    "@tauri-apps/plugin-os": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.3.3",
    "crypto-browserify": "^3.12.1",
    "json-diff-ts": "^4.8.2",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.6.2",
    "react-router-dom": "^7.6.2",
    "react-tooltip": "^5.29.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/finalhandler": "^1.2.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
