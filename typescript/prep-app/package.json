{
  "name": "prep-app",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "dist-electron/main.cjs",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"wait-on http://localhost:5173 && npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "build": "npm run build:renderer && npm run build:electron",
    "build:renderer": "tsc -b && vite build",
    "build:electron": "npm run build:main && npm run build:preload",
    "build:main": "esbuild src/electron/main/index.ts --bundle --platform=node --format=cjs --outfile=dist-electron/main.cjs --external:electron --external:electron-squirrel-startup --external:electron-reload",
    "build:preload": "esbuild src/electron/preload/index.ts --bundle --platform=node --format=cjs --outfile=dist-electron/preload.cjs --external:electron",
    "start": "npm run build && electron .",
    "dist": "npm run build && electron-builder",
    "dist:dir": "npm run build && electron-builder --dir",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@match-lock/shared": "file:../shared",
    "micromatch": "^4.0.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.6.2",
    "react-router-dom": "^7.6.2",
    "react-tooltip": "^5.29.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/micromatch": "^4.0.9",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12",
    "electron-squirrel-startup": "^1.0.1",
    "esbuild": "^0.24.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "wait-on": "^8.0.1"
  }
}
